@model web.Models.ViewChart

@{
    Layout = "";
}



@(ViewBag.ExceptionMessage != null ? Html.Raw("Sorry, an error occurred while processing your request: " + (String) ViewBag.ExceptionMessage) : Html.Raw(Url.Action("RenderSnapshot", (string) ViewBag.ControllerName, new {id = (Int64) ViewBag.ChartID, width = (Int64) ViewBag.Width, height = (Int64) ViewBag.Height})))

